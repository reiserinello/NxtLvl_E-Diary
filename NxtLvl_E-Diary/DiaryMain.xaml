<Window x:Class="NxtLvl_E_Diary.DiaryMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NxtLvl_E_Diary"
        mc:Ignorable="d"
        Title="DiaryMain" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="dtagrdDiaryEntrys" HorizontalAlignment="Left" Height="191" Margin="10,147,0,0" VerticalAlignment="Top" Width="259"/>
        <Button x:Name="btnNewEntry" Content="New" HorizontalAlignment="Left" Margin="274,147,0,0" VerticalAlignment="Top" Width="75" Click="btnNewEntry_Click"/>
        <Button x:Name="btnDeleteEntry" Content="Delete" HorizontalAlignment="Left" Margin="274,173,0,0" VerticalAlignment="Top" Width="75" Click="btnDeleteEntry_Click"/>
        <Button x:Name="btnEditEntry" Content="Edit" HorizontalAlignment="Left" Margin="274,199,0,0" VerticalAlignment="Top" Width="75" Click="btnEditEntry_Click"/>
    </Grid>
</Window>
